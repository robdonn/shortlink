  },
  "license": "MIT",
  "scripts": {
    "start": "nodemon server/server.js",
    "services:start": "docker-compose -f docker-compose.services.yml up",
    "services:delete": "docker-compose -f docker-compose.services.yml down -v",
    "build": "rimraf build && cross-env NODE_ENV=production webpack --config ./config/webpack/webpack.config.prod.js",
    "preview:start": "docker-compose -f docker-compose.services.yml -f docker-compose.preview.yml up",
    "preview:delete": "docker-compose -f docker-compose.services.yml -f docker-compose.preview.yml down -v",
    "preview:rebuild": "docker-compose -f docker-compose.services.yml -f docker-compose.preview.yml build --no-cache",
    "lint": "eslint .",
    "test:acceptance": "jest --config=config/jest/jest.config.acceptance.js",
    "test:unit": "jest --config=config/jest/jest.config.unit.js"
  },
  "dependencies": {
    "@reach/router": "^1.2.1",
    "body-parser": "^1.19.0",
