    "@testing-library/react": "^9.3.2",
    "@types/react": "^16.9.11",
    "autoprefixer": "^9.7.1",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "cross-env": "^6.0.3",
