    "mongoose": "^5.7.10",
    "morgan": "^1.9.1",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "shortid": "^2.2.15",
    "valid-url": "^1.0.9"
  },
